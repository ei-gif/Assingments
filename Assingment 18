#include<stdio.h> 
int main() { 
int num1,num2; 
printf("Enter the first number:"); 
scanf("%d\n", &num1); 
printf("Enter the second number:"); 
scanf("%d\n", &num2); 
if(num1 > num2) { 
printf ("%d is greater than %d\n", num1,num2);
} 
if(num2 > num1){ 
printf("%d is greater than %d\n", num1, num2); 
} 
return 0; 
} 